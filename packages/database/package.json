{
  "name": "@dealerbdc/database",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/schema/index.d.ts",
      "default": "./dist/schema/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "tsx src/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "import:csv": "tsx src/import/importCsv.ts",
    "build:aggregates": "tsx src/import/buildAggregates.ts",
    "clean": "rm -rf dist .turbo node_modules"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "drizzle-orm": "^0.29.3",
    "pg": "^8.11.5",
    "pg-copy-streams": "^6.0.5",
    "postgres": "^3.4.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/pg": "^8.11.10",
    "@types/pg-copy-streams": "^1.1.6",
    "@types/node": "^20.11.5",
    "drizzle-kit": "^0.20.10",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  }
}
